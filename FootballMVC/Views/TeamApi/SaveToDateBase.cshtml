    {
        <h1>Команда вже був збережена в базі даних</h1>
    }
    else
    {
        <h1>Збережено</h1>
    }
}
<div>
    <a asp-controller="TeamApi" asp-action="ViewTemsByLeagID" asp-route-id="@Model.Id">Назад</a>
</div>
